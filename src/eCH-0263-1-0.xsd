					<xs:documentation xml:lang="de">Mastpoulet, Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrSchMImpx">
				<xs:annotation>
					<xs:documentation xml:lang="de">Mastschweine, Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrSchMLin">
				<xs:annotation>
					<xs:documentation xml:lang="de">Mastschweine, Lineare Korrektur</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrSchZImpx">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zuchtschweine, Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrSchZLin">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zuchtschweine, Lineare Korrektur</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrTrutImpx">
				<xs:annotation>
					<xs:documentation xml:lang="de">Truten, Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
