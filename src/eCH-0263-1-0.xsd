	<xs:simpleType name="productFamilyType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Produktfamilie</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="FARM_MANURE_RECYCLING">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hof- und Recyclingdünger</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="PLANT_PROTECTION_PRODUCT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Pflanzenschutzmittel</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="FERTILISER">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dünger</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DRESSED_SEED">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gebeiztes Saatgut</xs:documentation>
				</xs:annotation>
