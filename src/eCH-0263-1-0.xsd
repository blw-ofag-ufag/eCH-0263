<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0263="http://www.ech.ch/xmlns/eCH-0263/1" xmlns:eCH-0010="http://www.ech.ch/xmlns/eCH-0010/7" xmlns:eCH-0044="http://www.ech.ch/xmlns/eCH-0044/4" xmlns:eCH-0097="http://www.ech.ch/xmlns/eCH-0097/6" xmlns:eCH-0261="http://www.ech.ch/xmlns/eCH-0261/1" xmlns:eCH-0262="http://www.ech.ch/xmlns/eCH-0262/1" xmlns:eCH-0265="http://www.ech.ch/xmlns/eCH-0265/1" targetNamespace="http://www.ech.ch/xmlns/eCH-0263/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:annotation>
		<xs:documentation xml:lang="de">Agrardaten - Betriebsmittel</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/7" schemaLocation="http://www.ech.ch/xmlns/eCH-0010/7/eCH-0010-7-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0044/4" schemaLocation="http://www.ech.ch/xmlns/eCH-0044/4/eCH-0044-4-1.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0097/6" schemaLocation="http://www.ech.ch/xmlns/eCH-0097/6/eCH-0097-6-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0261/1" schemaLocation="http://www.ech.ch/xmlns/eCH-0261/1/eCH-0261-1-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0265/1" schemaLocation="http://www.ech.ch/xmlns/eCH-0265/1/eCH-0265-1-0.xsd"/>
	<!--Hauptdatentypen-->
	<xs:complexType name="plantProtectionProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Pflanzenschutzmittel</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="admissonType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zuslassungsart</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CH_ONLY">
							<xs:annotation>
								<xs:documentation xml:lang="de">Nur in der Schweiz zugelassen</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="PARALLEL_IMPORT">
							<xs:annotation>
								<xs:documentation xml:lang="de">Parallelimport</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="privateUsageAdmissable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zulassung für Privatanwender</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plantProtectionProductCategory" type="eCH-0263:plantProtectionProductCategoryType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Pflanzenschutzmittel-Produktkategorie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="indication" type="eCH-0263:indicationType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Indikation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="permissionHolder" type="eCH-0097:uidStructureType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="permissionHolderAdress" type="eCH-0010:organisationMailAddressType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber (wenn UID unbekannt)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Pflanzenschutzmittels verwendet wird. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="l"/>
						<xs:enumeration value="pc"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/L. Die Dichte muss zwingend angegeben werden, falls «Liter» als Transaktionseinheit (transac-tionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="approvalDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum der Zulassung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hazardLabeling" type="eCH-0263:hazardLabelingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gefahrenkennzeichnung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="exhaustionDeadline" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Aufbrauchsfrist</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="soldOutDeadline" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ausverkaufsfrist</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="manureRecyclingProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Hof- und Recyclingdünger </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manureRecycling-ProductCategory">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hof- und Recyclingdünger-Produktkategorie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="FARM_MANURE">
							<xs:annotation>
								<xs:documentation xml:lang="de">Hofdünger</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="RECYCLING">
							<xs:annotation>
								<xs:documentation xml:lang="de">Recyclingdünger</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="typeOfManureRe-cyclingProduct" type="eCH-0263:typeOfManureRecyclingProductType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Produkttyp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Hofdüngers verwendet wird. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="l"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/L. Die Dichte muss zwingend angegeben werden, falls «Liter» als Transaktionseinheit (transac-tionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="matterState">
				<xs:annotation>
					<xs:documentation xml:lang="de">Aggregatszustand</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="SOLID">
							<xs:annotation>
								<xs:documentation xml:lang="de">Fest</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="LIQUID">
							<xs:annotation>
								<xs:documentation xml:lang="de">Flüssig</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="manufacturer" type="eCH-0097:namedOrganisationIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">BUR-Nummer des Herstellers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manureDerivation" type="eCH-0263:manureDerivationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ursprung des verwendeten Materials</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="laboratoryAnalysisDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Analysedatatum für betriebsspezifische Produkte</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validFromInclusive" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, ab wann der Datensatz gültig ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validToInclusive" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, bis zu welchem der Datensatz gültig ist.
Falls ein Eintrag nicht mehr existiert/geändert wurde, wird er mit dem Datum bis zu welchem er gültig war, versehen. Die Liste bleibt so rückwärtskompatibel
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fertiliserProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Dünger</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="fertiliserProductCategory" type="eCH-0263:fertiliserProductCategoryType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dünger-Produktkategorie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="permissionHolderUID" type="eCH-0097:uidStructureType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber Unternehmen (UID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="permissionHolderVn" type="eCH-0044:vnType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber natürliche Person (AHVN13)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="permissionHolderNamedPersonId" type="eCH-0044:namedPersonIdType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber natürliche Person (eIAM Login-Id)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="manufacturerUID" type="eCH-0097:uidStructureType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Hersteller Unternehmen (UID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="manufacturerVn" type="eCH-0044:vnType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Hersteller natürliche Person (AHVN13)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="manufacturerrNamedPersonId" type="eCH-0044:namedPersonIdType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Hersteller natürliche Person (eIAM Login Id)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Düngers verwendet wird.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="l"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/L. Die Dichte muss zwingend angegeben werden, falls «Liter» als Transaktionseinheit (transac-tionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleFromInclusive" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, ab welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleToInclusive" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, bis zu welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="feedstuffType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Futtermittel</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lotNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Chargen-Nr.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="feedstuffProductCategory">
				<xs:annotation>
					<xs:documentation xml:lang="de">Futtermittel Produktkategorie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="FODDER">
							<xs:annotation>
								<xs:documentation xml:lang="de">Grundfutter</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="CONCENTRATED_FODDER">
							<xs:annotation>
								<xs:documentation xml:lang="de">Kraftfutter</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="feedstuffPro-ductType" type="eCH-0263:feedstuffProductTypeType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Produkttyp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturer" type="eCH-0097:uidStructureType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hersteller</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="origin">
				<xs:annotation>
					<xs:documentation xml:lang="de">Herkunft</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CH">
							<xs:annotation>
								<xs:documentation xml:lang="de">Schweiz</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="OTHER_FOREIGN">
							<xs:annotation>
								<xs:documentation xml:lang="de">Sonstiges Ausland</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="label" type="eCH-0263:labelType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Label</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intendedForAnimal" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erlaubte Tierkategorien
Referenziert einen Code einer Spezies, welcher durch «taxonomySpecies» aus eCH-0266 definiert wird.
</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="organic" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angabe, ob ein Futtermittel die Bio-Richtlinien erfüllt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="laboratoryAnalysisDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Analysedatum für betriebsspezifische Produkte</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Futtermittels verwendet wird.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="l"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verfällt am</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Verkauf ab</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleFromInclusive" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, ab welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleToInclusive" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, bis zu welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="dressedSeedType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Gebeiztes Saatgut</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="retailName" type="eCH-0261:descriptorType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Handelsbezeichnung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="admissonType" type="eCH-0263:admissionTypeType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zulassungsart</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="seedTreatment" type="eCH-0261:identifierType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="de">Saatbehandlung</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="de">Inhaltsstoffe</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="transactionUnit">
