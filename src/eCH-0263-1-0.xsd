<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:eCH-0263="http://www.ech.ch/xmlns/eCH-0263/1" xmlns:eCH-0010="http://www.ech.ch/xmlns/eCH-0010/8" xmlns:eCH-0108="http://www.ech.ch/xmlns/eCH-0108/7" xmlns:eCH-0261="http://www.ech.ch/xmlns/eCH-0261/1" xmlns:eCH-0265="http://www.ech.ch/xmlns/eCH-0265/1" targetNamespace="http://www.ech.ch/xmlns/eCH-0263/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:annotation>
		<xs:documentation xml:lang="de">Agrardaten - Betriebsmittel</xs:documentation>
	</xs:annotation>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0010/8" schemaLocation="http://www.ech.ch/xmlns/eCH-0010/8/eCH-0010-8-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0108/7" schemaLocation="./eCH-0108-7-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0261/1" schemaLocation="../../eCH-0261/src/eCH-0261-1-0.xsd"/>
	<xs:import namespace="http://www.ech.ch/xmlns/eCH-0265/1" schemaLocation="../../eCH-0265/src/eCH-0265-1-0.xsd"/>
	<!--Hauptdatentypen-->
	<xs:complexType name="plantProtectionProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Pflanzenschutzmittel</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternateId" type="eCH-0261:identifierType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusätzliche Identifikatoren (z.B. W-Nummer)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="privateUsageAdmissable" type="xs:boolean">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zulassung für Privatanwender</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plantProtectionProductCategory" type="eCH-0263:plantProtectionProductCategoryType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Pflanzenschutzmittel-Produktkategorie</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="indication" type="eCH-0263:indicationType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Indikation</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="permissionHolder" type="eCH-0108:uidType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber (UID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="permissionHolderAdress" type="eCH-0010:organisationMailAddressType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Bewilligungsinhaber (wenn UID unbekannt)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Pflanzenschutzmittels verwendet wird. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="m3"/>
						<xs:enumeration value="pc"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/m3. Die Dichte muss zwingend angegeben werden, falls «Kubikmeter» als Transaktionseinheit (transactionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="approvalDate" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum der Zulassung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="hazardLabeling" type="eCH-0263:hazardLabelingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gefahrenkennzeichnung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" type="eCH-0261:validityType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angaben zur Gültigkeitsdauer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="manureRecyclingProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Hof- und Recyclingdünger </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternateId" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusätzliche Identifikatoren</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeOfProduct">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angabe, ob es sich um ein Standardprodukt, ein betriebsspezifisches Produkt oder ein Metaprodukt handelt.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="STANDARD">
							<xs:annotation>
								<xs:documentation xml:lang="de">Standardprodukt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="FACILITY_SPECIFIC">
							<xs:annotation>
								<xs:documentation xml:lang="de">Betriebsspezifisches Produkt </xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="META_PRODUCT">
							<xs:annotation>
								<xs:documentation xml:lang="de">Metaprodukt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="referenceToProduct" type="eCH-0261:identifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Standardprodukt, von welchem ein betriebsspezifisches Produkt abgeleitet ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manureRecyclingProductCategory">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hof- und Recyclingdünger-Produktkategorie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="FARM_MANURE">
							<xs:annotation>
								<xs:documentation xml:lang="de">Hofdünger</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="RECYCLING">
							<xs:annotation>
								<xs:documentation xml:lang="de">Recyclingdünger</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="typeOfManureRecyclingProduct" type="eCH-0263:typeOfManureRecyclingProductType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Produkttyp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Hofdüngers verwendet wird. </xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="m3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/m3. Die Dichte muss zwingend angegeben werden, falls «Kubikmeter» als Transaktionseinheit (transactionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="matterState">
				<xs:annotation>
					<xs:documentation xml:lang="de">Aggregatszustand</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="SOLID">
							<xs:annotation>
								<xs:documentation xml:lang="de">Fest</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="LIQUID">
							<xs:annotation>
								<xs:documentation xml:lang="de">Flüssig</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="manufacturer" type="eCH-0108:localIdType">
				<xs:annotation>
					<xs:documentation xml:lang="de">BUR-Nummer des Herstellers</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manureDerivation" type="eCH-0263:manureDerivationType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Ursprung des verwendeten Materials</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="metaIngredient" type="eCH-0263:metaIngredientType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="de">Meta-Inhaltsstoff</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="laboratoryAnalysisDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Analysedatatum für betriebsspezifische Produkte</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verificationDocumentId" type="eCH-0261:identifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">ID/Referenz auf ein Nachweisdokument. Dies wird für die betriebsspezifischen Mengen der Inhaltsstoffe des Produkts benötigt (z.B. Laboranalyse).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="validity" type="eCH-0261:validityType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angaben zur Gültigkeitsdauer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fertiliserProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Dünger</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternateId" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusätzliche Identifikatoren (z.B. CPID)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="fertiliserProductCategory" type="eCH-0263:fertiliserProductCategoryType">
					<xs:annotation>
						<xs:documentation xml:lang="de">TO DO! Anja fragen</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="fertiliserProductCategory2" type="eCH-0263:fertiliserProductCategoryType">
					<xs:annotation>
						<xs:documentation xml:lang="de">TO DO! Anja fragen</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:choice>
				<xs:element name="manufacturerUID" type="eCH-0108:uidType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Hersteller (Unternehmen mit UID)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="manufacturerPerson" type="eCH-0261:personIdentifierType">
					<xs:annotation>
						<xs:documentation xml:lang="de">Hersteller (natürliche Person)</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Düngers verwendet wird.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="m3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/m3. Die Dichte muss zwingend angegeben werden, falls «Kubikmeter» als Transaktionseinheit (transac-tionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ingredient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Inhaltsstoff</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleFromInclusive" type="xs:date">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, ab welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="saleToInclusive" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Datum, bis zu welchem der Verkauf zugelassen ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="version" type="xs:int">
				<xs:annotation>
					<xs:documentation xml:lang="de">Version</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="feedstuffType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Futtermittel</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="id" type="eCH-0261:identifierType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Identifikator</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="alternateId" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Zusätzliche Identifikatoren</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="description" type="eCH-0261:descriptorType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Beschreibung</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeOfProduct">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angabe, ob es sich um ein Standardpro-dukt, ein betriebsspezifisches Produkt oder ein Metaprodukt handelt.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="STANDARD">
							<xs:annotation>
								<xs:documentation xml:lang="de">Standardprodukt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="FACILITY_SPECIFIC">
							<xs:annotation>
								<xs:documentation xml:lang="de">Betriebsspezifisches Produkt </xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="META_PRODUCT">
							<xs:annotation>
								<xs:documentation xml:lang="de">Metaprodukt</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="referenceToProduct" type="eCH-0261:identifierType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Standardprodukt, von welchem ein betriebsspezifisches Produkt abgeleitet ist.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lotNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Chargen-Nr.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:maxLength value="50"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="feedstuffProductCategory">
				<xs:annotation>
					<xs:documentation xml:lang="de">Futtermittel Produktkategorie</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="FODDER">
							<xs:annotation>
								<xs:documentation xml:lang="de">Grundfutter</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="CONCENTRATED_FODDER">
							<xs:annotation>
								<xs:documentation xml:lang="de">Kraftfutter</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TypeOfFeedstuffProduct" type="eCH-0263:typeOfFeedstuffProductType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Produkttyp</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="manufacturer" type="eCH-0108:uidType">
				<xs:annotation>
					<xs:documentation xml:lang="de">Hersteller (UID)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="origin" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Herkunft</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CH">
							<xs:annotation>
								<xs:documentation xml:lang="de">Schweiz</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="FOREIGN">
							<xs:annotation>
								<xs:documentation xml:lang="de">Ausland</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="label" type="eCH-0263:labelType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Label</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="intendedForAnimal" type="eCH-0261:identifierType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="de">Erlaubte Tierkategorien
Referenziert einen Code einer Spezies, welcher durch «zoologicalAnimal» aus eCH-0266 definiert wird.
</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="organicRegulationConformity">
				<xs:annotation>
					<xs:documentation xml:lang="de">Angabe, ob ein Futtermittel die Bio-Richtlinien des Bundes erfüllt.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:token">
						<xs:enumeration value="CH_ORGANIC">
							<xs:annotation>
								<xs:documentation xml:lang="de">Schweiz</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="UNDEFINED">
							<xs:annotation>
								<xs:documentation xml:lang="de">Ausland</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="laboratoryAnalysisDate" type="xs:date" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Analysedatum für betriebsspezifische Produkte</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="transactionUnit">
				<xs:annotation>
					<xs:documentation xml:lang="de">Einheit, welche bei Transaktionen dieses Futtermittels verwendet wird.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="eCH-0261:unitType">
						<xs:enumeration value="kg"/>
						<xs:enumeration value="m3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="density" type="xs:decimal" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="de">Dichte (Raumgewicht) in kg/m3.  Die Dichte muss zwingend angegeben werden, falls «Kubikmeter» als Transaktionseinheit (transactionUnit) angegeben wird.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice>
				<xs:element name="nutrient" type="eCH-0263:ingredientType" maxOccurs="unbounded">
					<xs:annotation>
						<xs:documentation xml:lang="de">Nährstoffe</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="metaNutrient" type="eCH-0263:metaIngredientType" maxOccurs="unbounded">
