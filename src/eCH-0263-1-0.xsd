		<xs:sequence>
			<xs:element name="item" type="eCH-0261:enumeratedItemType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="typeOfFeedstuffProductType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Produkttyp Futtermittel</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="item" type="eCH-0261:enumeratedItemType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="crossResistancePatternType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Restistenzgruppe</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="item" type="eCH-0261:enumeratedItemType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="labelType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Label</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="item" type="eCH-0261:enumeratedItemType"/>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="admissionTypeType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Zulassungsart</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="PARALLEL_IMPORT">
				<xs:annotation>
					<xs:documentation xml:lang="de">Parallelimport</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CHE_ONLY">
				<xs:annotation>
					<xs:documentation xml:lang="de">Nur in der Schweiz bewilligt</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EMERGENCY_ADMISSION">
				<xs:annotation>
					<xs:documentation xml:lang="de">Notfallzulassung</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DRESSED_SEED">
				<xs:annotation>
					<xs:documentation xml:lang="de">Gebeiztes Saatgut</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="nprCodeType">
		<xs:annotation>
			<xs:documentation xml:lang="de">Stallsystem</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="nPrKanImpx">
				<xs:annotation>
					<xs:documentation xml:lang="de">Kaninchen Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="nPrJungImpx">
				<xs:annotation>
					<xs:documentation xml:lang="de">Junghennen Import-Export-Bilanz</xs:documentation>
				</xs:annotation>
